            android:name="qszhu.trakr.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="qszhu.trakr.task.TaskTimerActivity" />
        <activity android:name="qszhu.trakr.plan.SelectPlanActivity" />
        <activity android:name="qszhu.trakr.target.SelectTargetActivity" />
        <activity android:name="qszhu.parse.login.LoginActivity" />
